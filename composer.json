{
    "name": "nhalstead/php-facilitator",
    "description": "Trigger Webhooks to post data to channels in php. This will work with Slack, Discord, and other systems that use WebHooks.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Discord",
        "Slack",
        "Webhook",
        "Coordinator",
        "Client"
    ],
    "authors": [
        {
            "name": "Noah Halstead",
            "email": "nhalstead00@gmail.com",
            "homepage": "https://nhalstead.me"
        }
    ],
    "require": {
      "php": ">=7.0",
      "ext-curl": "*",
      "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "nhalstead\\Facilitator\\": "src"
        }
    },
    "support": {
        "issues": "https://github.com/nhalstead/php-facilitator/issues",
        "source": "https://github.com/nhalstead/php-facilitator/",
        "wiki": "https://github.com/nhalstead/php-facilitator/issues",
        "docs": "https://github.com/nhalstead/php-facilitator/wiki"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https:\/\/github.com\/nhalstead\/php-facilitator"
        }
    ]
}
